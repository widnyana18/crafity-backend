{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/crafity-api/src",
  "monorepo": true,
  "root": "apps/crafity-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/crafity-api/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/crafity-api",
      "entryFile": "main",
      "sourceRoot": "apps/crafity-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crafity-api/tsconfig.app.json"
      }
    },
    "artwork-agg": {
      "type": "application",
      "root": "apps/artwork-agg",
      "entryFile": "main",
      "sourceRoot": "apps/artwork-agg/src",
      "compilerOptions": {
        "tsConfigPath": "apps/artwork-agg/tsconfig.app.json"
      }
    },
    "artwork-per": {
      "type": "application",
      "root": "apps/artwork-per",
      "entryFile": "main",
      "sourceRoot": "apps/artwork-per/src",
      "compilerOptions": {
        "tsConfigPath": "apps/artwork-per/tsconfig.app.json"
      }
    },
    "auth-agg": {
      "type": "application",
      "root": "apps/auth-agg",
      "entryFile": "main",
      "sourceRoot": "apps/auth-agg/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-agg/tsconfig.app.json"
      }
    },
    "auth-per": {
      "type": "application",
      "root": "apps/auth-per",
      "entryFile": "main",
      "sourceRoot": "apps/auth-per/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-per/tsconfig.app.json"
      }
    },
    "user-agg": {
      "type": "application",
      "root": "apps/user-agg",
      "entryFile": "main",
      "sourceRoot": "apps/user-agg/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-agg/tsconfig.app.json"
      }
    },
    "user-per": {
      "type": "application",
      "root": "apps/user-per",
      "entryFile": "main",
      "sourceRoot": "apps/user-per/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-per/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  }
}
